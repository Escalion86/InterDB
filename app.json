{
  "expo": {
    "name": "IndividualCRM",
    "slug": "IndividualCRM",
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": ["android"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "icon": "./assets/gear96.png",
      "useNextNotificationsApi": true,
      "package": "com.escalion.Individual_CRM",
      "versionCode": 40,
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ],
      "googleServicesFile": "./google-services.json"
    }
  }
}
