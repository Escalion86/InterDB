{
  "expo": {
    "name": "iDB",
    "slug": "Individual_DataBase",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": ["android"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "icon": "./assets/gear96.png",
      "useNextNotificationsApi": true,
      "package": "com.escalion.iDB"
    }
  }
}
